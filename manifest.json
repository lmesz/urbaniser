{
  "manifest_version": 2,
  "name": "Urbaniser",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.0.min.js", "jquery-ui.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery-3.5.0.min.js", "background.js"]
  }
}